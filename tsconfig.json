    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "include": ["./src/"]
}
